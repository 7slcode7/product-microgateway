endpoint http:LoadBalanceClient {{qualifiedServiceName}}_{{endpointUrlType}} {
    targets: [
    {{#endpoints}}    {url: gateway:retrieveConfig("{{api.name}}.{{api.version}}.{{endpointUrlType}}.{{@index}}", "{{endpointUrl}}")}{{#unless @last}},
    {{/unless}}{{/endpoints}}
    ],
    algorithm: http:ROUND_ROBIN{{>caching}}{{>basicAuth}}
};